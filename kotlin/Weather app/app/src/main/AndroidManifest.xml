<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.googlemap">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="org.apache.http.legacy"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Kattegori"/>
        <activity android:name=".DrawerInfo" />
        <activity android:name=".WeatherForecast.WeatherForecast" />
        <activity android:name=".ocationForecast.nearby.BeachActivity" />
        <activity android:name=".ocationForecast.met.OcationForecast" />
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings" />
        <activity android:name=".ocationForecast.locationForecast.Korttid.Korttid" />
        <activity android:name=".ocationForecast.locationForecast.Langtid.Langtid" />
        <activity android:name=".category.CategoryFrag" />
        <activity android:name=".Clouds" />
        <activity android:name=".Temperatur.weatherIconActivity.WeatherIconActivity" />
        <activity android:name=".Temperatur.forhold.Forhold" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map_key" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".SplashScreen"
            android:noHistory="true"
            android:theme="@style/AppTheme.SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mainAct.MainActivity" />
    </application>

</manifest>